{
  "type": "module",
  "name": "et-calendar",
  "version": "1.1.0",
  "description": "A feature-rich React library that provides components, hooks, and utilities for working with both the Ethiopian and Gregorian calendars. It facilitates date selection, formatting, and conversion between these two calendars, making it easy to build applications that require dual calendar support.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "test": "jest",
    "watch:dev": "rollup -c -w --bundleConfigAsCjs",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "et-calendar",
    "react",
    "react-component",
    "datepicker",
    "datetimepicker",
    "calendar",
    "ethiopian calendar",
    "gregorian calendar"
  ],
  "author": {
    "name": "Gedion W"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gedion-01/et-calendar.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/Gedion-01/eth-calendar/issues"
  },
  "homepage": "https://github.com/Gedion-01/eth-calendar#readme",
  "dependencies": {
    "lucide-react": "^0.344.0",
    "luxon": "^3.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.3.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.0",
    "postcss-modules": "^4.3.0",
    "react": "^18.3.1",
    "rollup": "^3.20.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18 || ^19",
    "react-dom": "^16 || ^17 || ^18 || ^19"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./lib": {
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    }
  }
}
